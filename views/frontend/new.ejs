<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/new.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;1,200;1,300&display=swap" rel="stylesheet">
    <script src="index.js"></script>
</head>
<body>
    <div class="navbar">
        <div class="logo">
            <img src="/images/Logoremovedbackground.png" alt="logo">
        </div>
        <div class="middle">
            <ul>
                <li><a href="">Home</a></li>
                <li><a href="">Services</a></li>
                <li><a href="">Doctors</a></li>
                <li><a href="">Expertise</a></li>
            </ul>
        </div>
        <div class="right">
                <a href="/logout" class="btn-green mt-20px">
                    <i class="fas fa-calendar-week"></i>Book an appointment
                </a>
                <a href="/logout" class="btn-green mt-20px">
                    Logout
                </a>
        </div>
    </div>
    <section class="banner">
        <div class="banner-content">
            <h1> Our expertise at your service</h1>       
            </div>
        <video autoplay loop muted>
            <source src="/images/pexels-ron-lach-7579331 (2160p).mp4" type="video/mp4">
        </video>
    </section>
    <div class="service">
        <a href="javascript:void(0);">
            <div class="service-content">
                Our Service Providers
                <div class="service-icon">
                    <i class="fas fa-heartbeat"></i>
                </div>
            </div>

        </a>
        <a href="javascript:void(0);">
            <div class="service-content">
              Book an Appointment
              <div class="service-icon">
                  <i class="fas fa-calendar"></i>
              </div>
            </div>

        </a>
        <a href="javascript:void(0);">
            <div class="service-content">
                Have an emergency ?
                <div class="service-icon">
                        <i class="fas fa-phone-volume"></i>
                    </div>
                </div>
                <div class="call-number">
                    <p>Emergency Line</p>
                    <h3>123-456-789</h3>
                </div>

        </a>
       </div>

       <!-- WORKING HOURS -->

       <section class="work">
        <div class="container-fluid text-center">
            <div class="row">
               <div class="col-md-4">
                <div class="hours">
                    <h2>Working Hours</h2>
                    <div class="time">
                        <div class="booking">Mon - Wed</div>
                        <div class="booking">8AM - 7PM</div>
                        <div class="booking">
                            <button type="button" class="btn-white">
                                <i class="fas fa-calendar-week"> </i>Book
                            </button>
                        </div>
                    </div>
                    <div class="time">
                        <div class="booking">Thursday</div>
                        <div class="booking">8AM - 7PM</div>
                        <div class="booking">
                            <button type="button" class="btn-white">
                                <i class="fas fa-calendar-week"></i>Book
                            </button>
                        </div>
                    </div>
                    <div class="time">
                        <div class="booking">Friday</div>
                        <div class="booking">8AM - 7PM</div>
                        <div class="booking">
                            <button type="button" class="btn-white">
                                <i class="fas fa-calendar-week"></i>Book
                            </button>
                        </div>
                    </div>
                    <div class="time">
                        <div class="booking">Sat - Sunday</div>
                        <div class="booking">8AM - 7PM</div>
                        <div class="booking">
                            <button type="button" class="btn-white">
                                <i class="fas fa-calendar-week"></i>Book
                            </button>
                        </div>
                    </div>
                    <div class="time">
                        <h4>Time not Flexible?</h4>
                        <p></p>
                        <button type="button" class="btn-green btn-transparent mt-20px">
                            <i class="fas fa-calendar-week"></i>Suggest Checkup Time
                        </button>
        
                    </div>
                </div>
                
               </div>

               
    <script src="https://kit.fontawesome.com/6f77f484da.js" crossorigin="anonymous"></script> 
</body>
</html>